{
  "name": "stock-sentiment-desktop",
  "version": "1.0.0",
  "description": "Desktop app for Stock News Sentiment Trading Strategy",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && tsc -p tsconfig.electron.json",
    "preview": "vite preview",
    "electron:dev": "concurrently \"vite\" \"tsc -p tsconfig.electron.json --watch\" \"electron .\"",
    "start": "concurrently --kill-others \"vite --port 5173\" \"wait-on http://127.0.0.1:5173 && tsc -p tsconfig.electron.json && electron .\""
  },
  "dependencies": {
    "plotly.js-dist-min": "^2.27.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-plotly.js": "^2.6.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/react-plotly.js": "^2.6.0",
    "@vitejs/plugin-react": "^4.3.0",
    "concurrently": "^9.0.0",
    "electron": "^33.0.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.6.0",
    "vite": "^6.0.0",
    "wait-on": "^8.0.0"
  }
}
